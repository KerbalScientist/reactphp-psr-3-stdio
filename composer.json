{
    "name": "wyrihaximus/react-psr-3-stdio",
    "description": "Async PSR-3 stdio logger",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "clue/stdio-react": "^1.0",
        "psr/log": "^1.0",
        "wyrihaximus/psr-3-utilities": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\React\\PSR3\\Stdio\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\React\\Tests\\PSR3\\Stdio\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.0"
        }
    }
}
